<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student3" class="com.java.ex.Student">
		<constructor-arg value="세준"></constructor-arg>
		<constructor-arg value="25"></constructor-arg>
		<constructor-arg>
			<list>
				<value>코딩</value>
				<value>달리기</value>
			</list>
		</constructor-arg>
		
		<property name="height" value="173"></property>
		<property name="weight" value="65"></property>
	</bean>
	
	<bean id="family" class="com.java.ex.Family">
		<constructor-arg value="김종원"></constructor-arg>
		<constructor-arg value="이경희"></constructor-arg>
		
		<property name="sisName" value="김세인"></property>
		<property name="broName" value="김세찬"></property>
	</bean>
	

</beans>
